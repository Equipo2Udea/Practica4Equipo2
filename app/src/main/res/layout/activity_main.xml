<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <fragment
        android:id="@+id/details_fragment"
        class="com.example.practica4equipo2.Perimetro_cuadroFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_perimetro_cuadro" />
    <fragment
        android:id="@+id/details_fragment"
        class="com.example.practica4equipo2.Perimetro_TriangleFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_perimetro__triangle" />
    <fragment
        android:id="@+id/details_fragment"
        class="com.example.practica4equipo2.CuadradoFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_cuadrado"/>
</androidx.constraintlayout.widget.ConstraintLayout>